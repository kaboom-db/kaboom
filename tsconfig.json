{
  "compilerOptions": {
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": ["es2019", "dom"],
    "module": "es6",
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {"*": ["node_modules/*", "app/packs/*"]},
    "sourceMap": true,
    "noEmit": true,
    "strict": true
  },
  "exclude": ["node_modules", "vendor", "public"],
  "compileOnSave": false
}

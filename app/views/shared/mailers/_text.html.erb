<% paragraphs.each do |paragraph| %>
<table class="module" role="module" data-type="text" border="0" cellpadding="0" cellspacing="0" width="100%" style="table-layout:fixed">
  <tr>
    <td style="padding:10px 30px 10px 30px;line-height:22px;text-align:inherit" height="100%" valign="top" bgcolor>
    <div style="text-align:center">
      <span style="color:#333333;font-family: 'Montserrat',sans-serif;"><%= paragraph %></span>
    </div></td>
  </tr>
</table>
<% end %>
